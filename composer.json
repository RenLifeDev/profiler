{
    "name": "renlife/profiler-bundle",
    "description": "Profiler bundle for Symfony",
    "authors": [
        {
            "name": "Vadim Ashikhman",
            "email": "Vadim.Ashihman@Renlife.com"
        }
    ],
    "type": "bundle",
    "license": "proprietary",
    "require": {
        "php": "^7.1",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpstan/phpstan-shim": "^0.10.3",
        "phpstan/phpstan-strict-rules": "^0.10.1",
        "phpunit/phpunit": "^7.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Renlife\\ProfilerBundle\\": "src"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    }
}
